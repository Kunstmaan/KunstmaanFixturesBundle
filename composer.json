{
    "name": "kunstmaan/fixtures-bundle",
    "type": "symfony-bundle",
    "description": "With this bundle you'll be able to create fixtures through yaml files in an easy way",
    "keywords": [
        "kunstmaan",
        "fixtures",
        "yaml"
    ],
    "homepage": "https://github.com/Kunstmaan/FixturesBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan CMS Team",
            "homepage": "https://kunstmaancms.be",
            "email": "cms@kunstmaan.be"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2|^8.0",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "fakerphp/faker": "^1.15",
        "kunstmaan/admin-bundle": "^5.9|^6.0",
        "kunstmaan/media-bundle": "^5.9|^6.0",
        "kunstmaan/menu-bundle": "^5.9|^6.0",
        "kunstmaan/node-bundle": "^5.9|^6.0",
        "kunstmaan/pagepart-bundle": "^5.9|^6.0",
        "kunstmaan/utilities-bundle": "^5.9|^6.0",
        "symfony/mime": "^4.4"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "symfony/phpunit-bridge": "^5.1",
        "phpunit/phpunit": "^8.5"
    },
    "conflict": {
        "doctrine/persistence": "<1.3"
    },
    "autoload": {
        "psr-4": {
            "Kunstmaan\\FixturesBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.10-dev"
        }
    }
}
